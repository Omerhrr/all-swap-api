{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "netlify/functions/**/*",
    "routes/**/*",
    "config/**/*",
    "types/**/*",
    "services/**/*",
    "utils/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}